{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "out/**"]
    },
    "kosko#build": {
      "dependsOn": ["^build"],
      "outputs": ["deno.js"]
    }
  },
  "globalDependencies": ["api-extractor.json", "tsconfig.json"]
}
