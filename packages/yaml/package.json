{
  "name": "@kosko/yaml",
  "version": "1.0.2",
  "description": "Load Kubernetes YAML files in kosko.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/yaml",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko",
    "yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kosko/require": "workspace:*",
    "debug": "^4.3.3",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.6",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.5.12",
    "fetch-mock": "^9.11.0",
    "kubernetes-models": "^2.0.2",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.3"
  },
  "peerDependencies": {
    "kubernetes-models": ">=1.0.3"
  }
}
