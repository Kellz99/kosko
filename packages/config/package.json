  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/common-utils": "workspace:^",
    "@kosko/log": "workspace:^",
    "superstruct": "^0.15.3"
  },
  "devDependencies": {
    "@kosko/build-utils": "workspace:^",
    "@kosko/jest-preset": "workspace:^"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
